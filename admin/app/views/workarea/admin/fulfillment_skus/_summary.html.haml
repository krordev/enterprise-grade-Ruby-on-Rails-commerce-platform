= link_to fulfillment_sku_path(model), class: 'summary' do
  %span.summary__name= model.name
  .summary__info-container
    %span.summary__info= local_time_ago(model.updated_at)
    %span.summary__info= model.policy.humanize
  %span.summary__type{ title: t('workarea.admin.fulfillment_skus.summary.type') }= t('workarea.admin.fulfillment_skus.summary.type')
